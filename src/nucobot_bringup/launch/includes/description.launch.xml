<!--
  Robot description only (used in private and paired master).
 -->
<launch>
  <arg name="base"/>
  <arg name="3d_sensor"/>
  
  <arg name="urdf_file" default="$(find xacro)/xacro.py '$(find nucobot_description)/urdf/$(arg base)/$(arg base)_$(arg 3d_sensor).urdf.xacro'" />
  <param name="robot_description" command="$(arg urdf_file)" />

</launch>
